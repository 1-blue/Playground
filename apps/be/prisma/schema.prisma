generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum TodoType {
  TODO
  DOING
  DONE
}

model Cat {
  id     String  @id @default(uuid())
  name   String  @unique
  age    Int
  gender Boolean @default(true)
}

model Todo {
  id    String   @id @default(uuid())
  title String   @unique
  type  TodoType @default(TODO)
}
